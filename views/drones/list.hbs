<h2>Available drones</h2>

{{#each drones}}
  <div>
    <h3>{{name}}</h3>
    <p>Propellers: {{propellers}}</p>
    <p>Maximum Speed: {{maxSpeed}}</p>
    <a class="btn btn-warning" href="/drones/{{_id}}/edit">Edit</a>
    <p>
    <form action="/drones/{{_id}}/delete" method="POST">
      <button class="btn btn-danger">Delete</button>
    </form>
    </p>
  </div>
{{/each}}
